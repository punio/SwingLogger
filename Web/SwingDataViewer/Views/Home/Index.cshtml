@using SwingCommon
@model SwingDataViewer.Models.HomeViewModel
@{
    ViewData["Title"] = "Home Page";
}

<ul>
    @foreach (var logger in Model.Loggers)
    {
        <li>
            <a asp-controller="Home" asp-action="Graph" asp-route-id="@logger.Id" class="link">@logger.Name</a>
        </li>
    }
</ul>
